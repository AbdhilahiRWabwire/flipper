{
  "$schema" : "https://docs.renovatebot.com/renovate-schema.json",
  "extends" : [
    "config:best-practices",
    "group:all"
  ],
  "rebaseLabel" : "rebase",
  "labels" : [
    "dependencies"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "^com.squareup.anvil"
      ],
      "allowedVersions": "!/.*-1-8$/",
    },
    {
      "matchPackagePatterns": [
        "^com.vladsch.flexmark"
      ],
      "enabled": false
    }
  ]
}